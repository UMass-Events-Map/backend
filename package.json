{
  "name": "@umaps/backend",
  "version": "0.0.1",
  "description": "",
  "author": "mgpai22",
  "private": true,
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:bun": "bun run src/main.ts",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "queue:default": "node dist/src/queue/listenDefault.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:events": "jest --config ./test/jest-e2e.json events.e2e-spec.ts",
    "test:buildings": "jest --config ./test/jest-e2e.json buildings.e2e-spec.ts",
    "test:profiles": "jest --config ./test/jest-e2e.json profiles.e2e-spec.ts",
    "test:organizations": "jest --config ./test/jest-e2e.json organizations.e2e-spec.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration": "npm run typeorm -- migration:run -d src/database/data-source.ts"
  },
  "dependencies": {
    "@nestjs/bull": "^10.0.1",
    "@nestjs/common": "^10.0.5",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.5",
    "@nestjs/mapped-types": "*",
    "@nestjs/microservices": "^10.0.5",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.5",
    "@nestjs/platform-socket.io": "^10.0.5",
    "@nestjs/schedule": "^4.0.2",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/typeorm": "^10.0.2",
    "@nestjs/websockets": "^10.0.5",
    "@supabase/supabase-js": "^2.46.1",
    "axios": "^1.4.0",
    "bull": "^4.11.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "decimal.js": "^10.4.3",
    "discord.js": "^14.15.3",
    "dotenv": "^16.4.5",
    "json-bigint": "^1.0.0",
    "nestjs-pino": "^4.0.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.5",
    "pino-http": "^9.0.0",
    "pino-pretty": "^11.0.0",
    "queue-typescript": "^1.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "socket.io": "^4.7.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.4",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.5",
    "@types/bull": "^4.10.0",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.1",
    "@types/node": "18.16.12",
    "@types/passport": "^1.0.12",
    "@types/passport-jwt": "^3.0.9",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.8",
    "@types/uuid4": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.5.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}